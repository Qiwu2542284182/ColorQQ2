<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="#000"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <me.qiwu.colorqq.theme.SkinTitleBar
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="#12b7f5"
        android:tag="skin_color_title_immersive_bar" />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <me.qiwu.colorqq.theme.SkinView
            android:tag="skin_chat_background"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="54dp">
                <me.qiwu.colorqq.theme.SkinView
                    android:tag="skin_header_bar_bg.9"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <me.qiwu.colorqq.theme.SkinView
                    android:tag="skin_header_btn_back_normal.9"
                    android:layout_marginLeft="10dp"
                    android:layout_centerVertical="true"
                    android:layout_width="40dp"
                    android:layout_height="50dp"/>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_centerInParent="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <me.qiwu.colorqq.theme.SkinTextView
                        android:tag="skin_bar_text"
                        android:text="浪里个浪"
                        android:textSize="18sp"
                        android:textColor="#fff"
                        android:layout_gravity="center_horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <me.qiwu.colorqq.theme.SkinTextView
                        android:tag="skin_bar_text"
                        android:text="在线"
                        android:textSize="12sp"
                        android:textColor="#fff"
                        android:layout_gravity="center_horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                </LinearLayout>
                <me.qiwu.colorqq.theme.SkinAssetImageView
                    android:layout_toLeftOf="@id/single"
                    android:tag="skin_aio_head_twocall"
                    android:scaleType="center"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="5dp"
                    android:scaleY="1.5"
                    android:scaleX="1.5"
                    android:layout_width="30dp"
                    android:layout_height="30dp"/>
                <me.qiwu.colorqq.theme.SkinAssetImageView
                    android:id="@+id/single"
                    android:scaleType="center"
                    android:tag="skin_header_icon_single"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="5dp"
                    android:scaleY="1.5"
                    android:scaleX="1.5"
                    android:layout_width="30dp"
                    android:layout_height="30dp"/>
            </RelativeLayout>
            <LinearLayout
                android:layout_marginTop="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <me.qiwu.colorqq.library.RCLayout.RCImageView
                    android:tag="circle"
                    android:src="@drawable/head1"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="10dp"
                    android:layout_gravity="center_vertical"
                    android:layout_width="40dp"
                    android:layout_height="40dp"/>
                <me.qiwu.colorqq.theme.SkinFrameLayout
                    android:tag="skin_aio_friend_bubble_nor.9"
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <me.qiwu.colorqq.theme.SkinTextView
                        android:textColor="#000"
                        android:text="去不去吃饭"
                        android:gravity="center"
                        android:textSize="16dp"
                        android:layout_gravity="center"
                        android:tag="skin_chat_buble"
                        android:padding="20dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                </me.qiwu.colorqq.theme.SkinFrameLayout>
            </LinearLayout>
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_gravity="right"
                    android:layout_marginTop="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <me.qiwu.colorqq.theme.SkinFrameLayout
                        android:layout_gravity="center_vertical"
                        android:tag="skin_aio_user_bubble_nor.9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <me.qiwu.colorqq.theme.SkinTextView
                            android:textColor="#fff"
                            android:text="不了不了"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="16dp"
                            android:padding="20dp"
                            android:tag="skin_chat_buble_mine"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </me.qiwu.colorqq.theme.SkinFrameLayout>
                    <me.qiwu.colorqq.library.RCLayout.RCImageView
                        android:tag="circle"
                        android:src="@drawable/head2"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="16dp"
                        android:layout_width="40dp"
                        android:layout_height="40dp"/>
                </LinearLayout>
            </FrameLayout>

        </LinearLayout>
        <me.qiwu.colorqq.theme.SkinView
            android:layout_above="@id/pancel"
            android:layout_alignTop="@id/input"
            android:tag="skin_aio_input_bar_bg_theme_version2.9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <LinearLayout
            android:id="@+id/input"
            android:paddingTop="8dp"
            android:layout_above="@id/pancel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <me.qiwu.colorqq.theme.SkinFrameLayout
                android:layout_marginLeft="14dp"
                android:minHeight="36dp"
                android:layout_gravity="center_vertical"
                android:layout_marginRight="5dp"
                android:tag="skin_aio_input_bg.9"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"/>
            <me.qiwu.colorqq.theme.SkinFrameLayout
                android:tag="skin_aio_send_button_disabled.9"
                android:layout_marginRight="7dp"
                android:layout_gravity="center_vertical"
                android:layout_width="60dp"
                android:layout_height="36dp">
                <me.qiwu.colorqq.theme.SkinTextView
                    android:text="发送"
                    android:textSize="14dp"
                    android:textColor="#fff"
                    android:tag="skin_aio_send_button"
                    android:gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
            </me.qiwu.colorqq.theme.SkinFrameLayout>

        </LinearLayout>
        <me.qiwu.colorqq.theme.SkinView
            android:layout_alignParentBottom="true"
            android:tag="skin_aio_panel_icon_bg.9"
            android:layout_width="match_parent"
            android:layout_height="50dp"/>
        <LinearLayout
            android:id="@+id/pancel"
            android:layout_alignParentBottom="true"
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <me.qiwu.colorqq.theme.SkinView
                    android:tag="skin_aio_panel_ptt_nor"
                    android:layout_gravity="center"
                    android:layout_width="30dp"
                    android:layout_height="30dp"/>
            </FrameLayout>
            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <me.qiwu.colorqq.theme.SkinView
                    android:tag="skin_aio_panel_image_nor"
                    android:layout_gravity="center"
                    android:layout_width="30dp"
                    android:layout_height="30dp"/>
            </FrameLayout>
            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <me.qiwu.colorqq.theme.SkinView
                    android:tag="skin_aio_panel_camera_nor"
                    android:layout_gravity="center"
                    android:layout_width="30dp"
                    android:layout_height="30dp"/>
            </FrameLayout>
            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <me.qiwu.colorqq.theme.SkinView
                    android:tag="skin_aio_panel_hongbao_nor"
                    android:layout_gravity="center"
                    android:layout_width="30dp"
                    android:layout_height="30dp"/>
            </FrameLayout>
            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <me.qiwu.colorqq.theme.SkinView
                    android:tag="skin_aio_panel_emotion_nor"
                    android:layout_gravity="center"
                    android:layout_width="30dp"
                    android:layout_height="30dp"/>
            </FrameLayout>
            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <me.qiwu.colorqq.theme.SkinView
                    android:tag="skin_aio_panel_plus_nor"
                    android:layout_gravity="center"
                    android:layout_width="30dp"
                    android:layout_height="30dp"/>
            </FrameLayout>
        </LinearLayout>
    </RelativeLayout>

</LinearLayout>