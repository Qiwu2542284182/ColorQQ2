<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <me.qiwu.colorqq.theme.SkinAssetImageView
        android:scaleType="centerCrop"
        android:tag="qq_setting_me_bg"
        android:background="#22000000"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <LinearLayout
        android:id="@+id/qq_settine_me_head"
        android:orientation="horizontal"
        android:layout_marginTop="91dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <me.qiwu.colorqq.library.RCLayout.RCImageView
            android:src="@drawable/head1"
            android:tag="circle"
            android:layout_marginLeft="22dp"
            android:layout_width="60dp"
            android:layout_height="60dp"/>
        <LinearLayout
            android:layout_marginLeft="15dp"
            android:layout_gravity="center_vertical"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <me.qiwu.colorqq.theme.SkinTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="落地的企鹅"
                android:textSize="18sp"
                android:textColor="#fff"
                android:tag="qq_setting_me_title_color"/>
            <me.qiwu.colorqq.theme.SkinTextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="飞啊飞啊我的骄傲放纵"
                android:textSize="14sp"
                android:textColor="#fff"
                android:tag="qq_setting_me_sig_color"/>
        </LinearLayout>
    </LinearLayout>
    <me.qiwu.colorqq.theme.SkinAssetImageView
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:scaleType="fitXY"
        android:tag="qq_setting_me_qr_code_icon"
        android:layout_alignParentRight="true"
        android:layout_marginTop="91dp"
        android:layout_marginRight="15dp"/>
    <me.qiwu.colorqq.theme.SkinAssetImageView
        android:layout_width="23dp"
        android:layout_height="23dp"
        android:scaleType="fitXY"
        android:tag="qq_setting_me_back"
        android:layout_alignParentRight="true"
        android:layout_marginTop="29dp"
        android:layout_marginRight="15dp"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="19dp"
        android:paddingBottom="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp">
         <RelativeLayout
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginRight="12dp">
            <me.qiwu.colorqq.theme.SkinAssetImageView
                android:layout_width="28dp"
                android:layout_height="28dp"
                android:tag="qq_setting_sign_in"
                android:scaleType="centerCrop"
                android:layout_centerInParent="true" />
        </RelativeLayout>
        <me.qiwu.colorqq.theme.SkinTextView
            android:textSize="16dp"
            android:textColor="#000"
            android:tag="qq_setting_me_title_color"
            android:ellipsize="end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="打卡"
            android:singleLine="true" />
    </LinearLayout>
    <ScrollView
        android:layout_margin="20dp"
        android:scrollbars="none"
