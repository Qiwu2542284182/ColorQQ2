    <LinearLayout
        android:id="@+id/qq_setting_me_setting"
        android:layout_marginBottom="15dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="100dp"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <FrameLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_weight="1.0"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <me.qiwu.colorqq.theme.SkinAssetImageView
                    android:scaleType="centerCrop"
                    android:layout_gravity="center_vertical"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:tag="qq_setting_setting_white"/>
                <me.qiwu.colorqq.theme.SkinTextView
                    android:textSize="12dp"
                    android:duplicateParentState="true"
                    android:layout_marginTop="4dp"
                    android:text="设置"
                    android:layout_gravity="center_horizontal"
                    android:textColor="#232323"
                    android:tag="qq_setting_me_nightmode_color_white"
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"/>
            </LinearLayout>
        </FrameLayout>

        <FrameLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_weight="1.0"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <me.qiwu.colorqq.theme.SkinAssetImageView
                    android:scaleType="centerCrop"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:tag="qq_setting_me_nightmode_off_white"/>
                <me.qiwu.colorqq.theme.SkinTextView
                    android:textSize="12dp"
                    android:duplicateParentState="true"
                    android:layout_marginTop="4dp"
                    android:text="夜间"
                    android:textColor="#232323"
                    android:tag="qq_setting_me_nightmode_color_white"
                    android:gravity="center"
                    android:layout_gravity="center_horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"/>
            </LinearLayout>
        </FrameLayout>

        <FrameLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_weight="1.0"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <me.qiwu.colorqq.theme.SkinAssetImageView
                    android:scaleType="centerCrop"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
