                    android:layout_weight="1"
                    android:textSize="16sp"
                    android:text="我的文件"
                    android:textColor="#000"
                    android:tag="qq_setting_me_nightmode_color_white"/>
                <me.qiwu.colorqq.theme.SkinAssetImageView
                    android:scaleType="fitXY"
                    android:tag="skin_icon_arrow_right_normal_theme_version3"
                    android:layout_width="7dp"
                    android:layout_height="11.5dp"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="47dp">
                <me.qiwu.colorqq.theme.SkinAssetImageView
                    android:scaleType="fitXY"
                    android:tag="qq_setting_freetraffic_white"
                    android:layout_width="32dp"
                    android:layout_height="32dp"/>
                <me.qiwu.colorqq.theme.SkinTextView
                    android:layout_width="0dp"
                    android:layout_marginLeft="15dp"
                    android:layout_height="wrap_content"
